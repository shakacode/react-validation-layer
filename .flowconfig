suppress_comment=\\(.\\|\n\\)*\\$FlowIgnoreMe\\($\\|[^(]\\|(\\(>=0\\.\\(1[0-6]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*www[a-z,_]*\\)?)\\)

suppress_type=$FlowIssue
